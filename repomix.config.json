// npx -y repomix --config repomix.config.json --output repomix-output.md --style markdown --verbose
{
  "input": {},
  "include": [
    "src/app/routes/blueprint/tabs/tasks/**/*.ts",
    "src/app/routes/blueprint/tabs/tasks/**/*.html",
    "src/app/routes/blueprint/tabs/tasks/**/*.less",
    "angular.json",
    "package.json",
    "tsconfig.json"
  ],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/src/assets/**",
    "src/app/routes/data-v/**",
    "src/app/routes/delon/**",
    "src/app/routes/exception/**",
    "src/app/routes/extras/**",
    "src/app/routes/pro/**",
    "src/app/routes/style/**",
    "**/*.md"
  ],
  "ignore": {},
  "output": {
    "compress": false,
    "removeComments": false,
    "removeEmptyLines": false,
    "fileSummary": true,
    "directoryStructure": true,
    "files": true,
    "showLineNumbers": true,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 100,
      "includeLogsCount": 50
    }
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {}
}
