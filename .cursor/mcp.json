{
  "mcpServers": {
    "Chrome DevTools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "disabled": false
    },
    "Context7": {
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "ctx7sk-a6b61fdc-d440-4e26-bcb0-7fd6807c4787"
      },
      "retry": {
        "maxRetries": null,
        "retryDelay": 1000,
        "backoff": "exponential",
        "maxRetryDelay": 10000
      },
      "reconnect": {
        "enabled": true,
        "maxReconnectAttempts": 10,
        "reconnectDelay": 2000,
        "maxReconnectDelay": 30000
      }
    },
    "Playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "Sequential Thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false
    },
    "Software Planning Tool": {
      "command": "npx",
      "args": [
        "-y",
        "github:NightTrek/Software-planning-mcp"
      ],
      "disabled": false
    },
    "codacy": {
      "command": "npx",
      "args": [
        "-y",
        "@codacy/codacy-mcp@latest"
      ],
      "disabled": false
    },
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "disabled": false
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "./"
      ],
      "disabled": false
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory@latest"
      ],
      "env": {
        "MEMORY_FILE_PATH": "./memory.json"
      },
      "disabled": false
    },
    "mcp-server-ngzorro": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-ngzorro"
      ]
    },
    "repomix": {
      "command": "npx",
      "args": [
        "-y",
        "repomix",
        "--mcp"
      ],
      "disabled": false
    },
    "supabase": {
      "url": "https://mcp.supabase.com/mcp?project_ref=pfxxjtvnqptdvjfakotc",
      "headers": {}
    },
    "mcp-deepwiki": {
      "command": "npx",
      "args": ["-y", "mcp-deepwiki@latest"]
    },
    "retry": {
      "maxRetries": null,
      "retryDelay": 1000,
      "backoff": "exponential",
      "maxRetryDelay": 10000
    },
    "reconnect": {
      "enabled": true,
      "maxReconnectAttempts": 10,
      "reconnectDelay": 2000,
      "maxReconnectDelay": 30000
    }
  }
}